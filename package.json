{
    "private": true,
    "name": "yoga-by-delphine",
    "description": "Yoga by Delphine website",
    "version": "0.1.0",
    "author": "Nicolas Erny",
    "sideEffects": false,
    "scripts": {
        "build": "pnpm run generateBlurDataUrls && remix build",
        "predev": "rimraf ./.netlify/edge-functions/",
        "dev": "remix dev --manual -c \"ntl dev --framework=#static\"",
        "start": "netlify serve",
        "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint . --max-warnings 0",
        "typecheck": "tsc --noEmit",
        "generateBlurDataUrls": "tsx ./tools/generateBlurDataUrls.ts ./app/generated/blurDataUrls.ts"
    },
    "dependencies": {
        "@netlify/edge-functions": "^2.2.0",
        "@netlify/remix-edge-adapter": "^3.1.0",
        "@reach/dialog": "^0.18.0",
        "@netlify/remix-runtime": "^2.1.0",
        "@remix-run/react": "^2.2.0",
        "@remix-run/serve": "^2.2.0",
        "cloudinary-build-url": "^0.2.4",
        "clsx": "^2.0.0",
        "cross-env": "^7.0.3",
        "date-fns": "^2.30.0",
        "isbot": "^3.7.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "source-map-support": "^0.5.21",
        "tiny-invariant": "^1.3.1"
    },
    "devDependencies": {
        "@cld-apis/types": "^0.1.6",
        "@netlify/plugin-lighthouse": "^5.0.0",
        "@remix-run/dev": "^2.2.0",
        "@remix-run/serve": "^2.2.0",
        "@remix-run/eslint-config": "^2.2.0",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@types/source-map-support": "^0.5.10",
        "eslint": "^8.53.0",
        "husky": "^8.0.3",
        "prettier": "^3.0.3",
        "prettier-plugin-tailwindcss": "^0.5.7",
        "pretty-quick": "^3.1.3",
        "rimraf": "^5.0.5",
        "tailwindcss": "^3.3.5",
        "tsx": "^4.1.1",
        "typescript": "^5.2.2"
    },
    "overrides": {
        "react": "$react",
        "react-dom": "$react-dom"
    },
    "packageManager": "pnpm@8.9.2",
    "keywords": [
        "remix",
        "yoga",
        "courses",
        "shopify"
    ],
    "license": "MIT",
    "engines": {
        "node": ">=18"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nicolaserny/yoga-by-delphine"
    },
    "bugs": {
        "url": "https://github.com/nicolaserny/yoga-by-delphine/issues"
    },
    "eslintIgnore": [
        "node_modules",
        "coverage",
        "build",
        "public/build"
    ]
}
