{
    "private": true,
    "name": "yoga-by-delphine",
    "description": "Yoga by Delphine website",
    "version": "0.1.0",
    "author": "Nicolas Erny",
    "sideEffects": false,
    "type": "module",
    "scripts": {
        "prepare": "husky",
        "build": "pnpm run generateBlurDataUrls && react-router build",
        "dev": "react-router dev",
        "start": "pnpm run dev",
        "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint . --max-warnings 0",
        "typecheck": "react-router typegen && tsc --noEmit",
        "generateBlurDataUrls": "tsx ./tools/generateBlurDataUrls.ts ./app/generated/blurDataUrls.ts"
    },
    "dependencies": {
        "@headlessui/react": "^2.2.9",
        "@react-router/node": "^7.9.6",
        "@react-router/serve": "^7.9.6",
        "cloudinary-build-url": "0.2.4",
        "clsx": "2.1.1",
        "cross-env": "10.1.0",
        "date-fns": "4.1.0",
        "isbot": "5.1.32",
        "react": "19.2.0",
        "react-dom": "19.2.0",
        "react-router": "^7.9.6",
        "source-map-support": "0.5.21",
        "tiny-invariant": "1.3.3"
    },
    "devDependencies": {
        "@cld-apis/types": "0.1.6",
        "@netlify/plugin-lighthouse": "6.0.4",
        "@netlify/vite-plugin-react-router": "^2.1.2",
        "@react-router/dev": "^7.9.6",
        "@tailwindcss/aspect-ratio": "0.4.2",
        "@tailwindcss/vite": "^4.1.17",
        "@types/react": "19.2.5",
        "@types/react-dom": "19.2.3",
        "@types/source-map-support": "0.5.10",
        "eslint": "9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import-x": "^4.16.1",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "globals": "^16.5.0",
        "husky": "9.1.7",
        "prettier": "3.6.2",
        "prettier-plugin-tailwindcss": "0.7.1",
        "pretty-quick": "4.2.2",
        "rimraf": "6.1.0",
        "tailwindcss": "4.1.17",
        "tsx": "4.20.6",
        "typescript": "5.9.3",
        "typescript-eslint": "^8.46.4",
        "vite": "7.2.2",
        "vite-tsconfig-paths": "^5.1.4"
    },
    "packageManager": "pnpm@10.22.0",
    "keywords": [
        "remix",
        "yoga",
        "courses",
        "shopify"
    ],
    "license": "MIT",
    "engines": {
        "node": "22.x"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nicolaserny/yoga-by-delphine"
    },
    "bugs": {
        "url": "https://github.com/nicolaserny/yoga-by-delphine/issues"
    }
}
