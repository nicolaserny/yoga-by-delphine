{
    "private": true,
    "name": "yoga-by-delphine",
    "description": "Yoga by Delphine website",
    "version": "0.1.0",
    "author": "Nicolas Erny",
    "sideEffects": false,
    "scripts": {
        "build": "run-s \"build:*\"",
        "build:css": "pnpm run generate:css --minify",
        "build:remix": "remix build",
        "dev": "run-p \"dev:*\"",
        "dev:css": "pnpm run generate:css --watch",
        "dev:remix": "cross-env NODE_ENV=development netlify dev",
        "generate:css": "pnpm exec tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
        "start": "cross-env NODE_ENV=production netlify dev",
        "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint . --max-warnings 0",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@netlify/functions": "^1.4.0",
        "@reach/dialog": "^0.18.0",
        "@remix-run/netlify": "^1.15.0",
        "@remix-run/node": "^1.15.0",
        "@remix-run/react": "^1.15.0",
        "cloudinary-build-url": "^0.2.4",
        "clsx": "^1.2.1",
        "cross-env": "^7.0.3",
        "date-fns": "^2.29.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tiny-invariant": "^1.3.1"
    },
    "devDependencies": {
        "@cld-apis/types": "^0.1.6",
        "@remix-run/dev": "^1.15.0",
        "@remix-run/eslint-config": "^1.15.0",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@types/react": "^18.0.38",
        "@types/react-dom": "^18.0.11",
        "eslint": "^8.39.0",
        "husky": "^8.0.3",
        "netlify-plugin-inline-functions-env": "^1.0.9",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.7",
        "prettier-plugin-tailwindcss": "^0.2.7",
        "pretty-quick": "^3.1.3",
        "tailwindcss": "^3.3.1",
        "typescript": "^5.0.4"
    },
    "overrides": {
        "react": "$react",
        "react-dom": "$react-dom"
    },
    "packageManager": "pnpm@8.3.1",
    "keywords": [
        "remix",
        "yoga",
        "courses",
        "shopify"
    ],
    "license": "MIT",
    "engines": {
        "node": ">=16"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nicolaserny/yoga-by-delphine"
    },
    "bugs": {
        "url": "https://github.com/nicolaserny/yoga-by-delphine/issues"
    },
    "eslintIgnore": [
        "node_modules",
        "coverage",
        "build",
        "public/build"
    ]
}
