{
    "private": true,
    "name": "yoga-by-delphine",
    "description": "Yoga by Delphine website",
    "version": "0.1.0",
    "author": "Nicolas Erny",
    "sideEffects": false,
    "type": "module",
    "scripts": {
        "prepare": "husky",
        "build": "pnpm run generateBlurDataUrls && remix build",
        "predev": "rimraf ./.netlify/edge-functions/",
        "dev": "remix dev",
        "start": "netlify serve",
        "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint . --max-warnings 0",
        "typecheck": "tsc --noEmit",
        "generateBlurDataUrls": "tsx ./tools/generateBlurDataUrls.ts ./app/generated/blurDataUrls.ts"
    },
    "dependencies": {
        "@netlify/edge-functions": "2.11.0",
        "@netlify/remix-edge-adapter": "3.4.2",
        "@netlify/remix-runtime": "2.3.1",
        "@reach/dialog": "0.18.0",
        "@remix-run/react": "2.13.1",
        "@remix-run/serve": "2.13.1",
        "cloudinary-build-url": "0.2.4",
        "clsx": "2.1.1",
        "cross-env": "7.0.3",
        "date-fns": "4.1.0",
        "isbot": "5.1.17",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "source-map-support": "0.5.21",
        "tiny-invariant": "1.3.3"
    },
    "devDependencies": {
        "@cld-apis/types": "0.1.6",
        "@netlify/plugin-lighthouse": "6.0.1",
        "@remix-run/dev": "2.13.1",
        "@remix-run/serve": "2.13.1",
        "@tailwindcss/aspect-ratio": "0.4.2",
        "@types/react": "18.3.12",
        "@types/react-dom": "18.3.1",
        "@types/source-map-support": "0.5.10",
        "eslint": "9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import-x": "^4.4.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "globals": "^15.11.0",
        "husky": "9.1.6",
        "netlify-cli": "17.37.2",
        "prettier": "3.3.3",
        "prettier-plugin-tailwindcss": "0.6.8",
        "pretty-quick": "4.0.0",
        "rimraf": "6.0.1",
        "tailwindcss": "3.4.14",
        "tsx": "4.19.2",
        "typescript": "5.6.3",
        "typescript-eslint": "^8.12.2"
    },
    "overrides": {
        "react": "$react",
        "react-dom": "$react-dom"
    },
    "packageManager": "pnpm@9.10.0",
    "keywords": [
        "remix",
        "yoga",
        "courses",
        "shopify"
    ],
    "license": "MIT",
    "engines": {
        "node": "20.x"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nicolaserny/yoga-by-delphine"
    },
    "bugs": {
        "url": "https://github.com/nicolaserny/yoga-by-delphine/issues"
    }
}
