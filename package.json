{
    "private": true,
    "name": "yoga-by-delphine",
    "description": "Yoga by Delphine website",
    "version": "0.1.0",
    "author": "Nicolas Erny",
    "sideEffects": false,
    "type": "module",
    "scripts": {
        "prepare": "husky",
        "build": "pnpm run generateBlurDataUrls && remix vite:build",
        "predev": "rimraf ./.netlify/edge-functions/",
        "dev": "remix vite:dev",
        "start": "netlify serve",
        "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint . --max-warnings 0",
        "typecheck": "tsc --noEmit",
        "generateBlurDataUrls": "tsx ./tools/generateBlurDataUrls.ts ./app/generated/blurDataUrls.ts"
    },
    "dependencies": {
        "@headlessui/react": "^2.2.7",
        "@netlify/edge-functions": "2.17.0",
        "@netlify/remix-edge-adapter": "3.4.3",
        "@netlify/remix-runtime": "2.3.1",
        "@remix-run/react": "2.17.0",
        "@remix-run/serve": "2.17.0",
        "cloudinary-build-url": "0.2.4",
        "clsx": "2.1.1",
        "cross-env": "7.0.3",
        "date-fns": "4.1.0",
        "isbot": "5.1.29",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "source-map-support": "0.5.21",
        "tiny-invariant": "1.3.3"
    },
    "devDependencies": {
        "@cld-apis/types": "0.1.6",
        "@netlify/plugin-lighthouse": "6.0.1",
        "@remix-run/dev": "2.17.0",
        "@remix-run/fs-routes": "^2.17.0",
        "@remix-run/route-config": "^2.17.0",
        "@remix-run/serve": "2.17.0",
        "@tailwindcss/aspect-ratio": "0.4.2",
        "@tailwindcss/vite": "^4.1.11",
        "@types/react": "18.3.23",
        "@types/react-dom": "18.3.7",
        "@types/source-map-support": "0.5.10",
        "eslint": "9.32.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import-x": "^4.16.1",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^5.5.3",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "globals": "^15.15.0",
        "husky": "9.1.7",
        "netlify-cli": "18.1.0",
        "prettier": "3.6.2",
        "prettier-plugin-tailwindcss": "0.6.14",
        "pretty-quick": "4.2.2",
        "rimraf": "6.0.1",
        "tailwindcss": "4.1.11",
        "tsx": "4.20.3",
        "typescript": "5.8.2",
        "typescript-eslint": "^8.38.0",
        "vite": "6.3.5",
        "vite-tsconfig-paths": "^5.1.4"
    },
    "overrides": {
        "react": "$react",
        "react-dom": "$react-dom"
    },
    "packageManager": "pnpm@9.10.0",
    "keywords": [
        "remix",
        "yoga",
        "courses",
        "shopify"
    ],
    "license": "MIT",
    "engines": {
        "node": "20.x"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nicolaserny/yoga-by-delphine"
    },
    "bugs": {
        "url": "https://github.com/nicolaserny/yoga-by-delphine/issues"
    },
    "pnpm": {
        "patchedDependencies": {
            "@remix-run/server-runtime@2.17.0": "patches/@remix-run__server-runtime@2.17.0.patch"
        }
    }
}
