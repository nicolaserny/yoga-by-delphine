{
    "private": true,
    "name": "yoga-by-delphine",
    "description": "Yoga by Delphine website",
    "version": "0.1.0",
    "author": "Nicolas Erny",
    "sideEffects": false,
    "scripts": {
        "prepare": "husky",
        "build": "pnpm run generateBlurDataUrls && remix build",
        "predev": "rimraf ./.netlify/edge-functions/",
        "dev": "remix dev",
        "start": "netlify serve",
        "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint . --max-warnings 0",
        "typecheck": "tsc --noEmit",
        "generateBlurDataUrls": "tsx ./tools/generateBlurDataUrls.ts ./app/generated/blurDataUrls.ts"
    },
    "dependencies": {
        "@netlify/edge-functions": "2.3.1",
        "@netlify/remix-edge-adapter": "3.1.0",
        "@netlify/remix-runtime": "2.1.0",
        "@reach/dialog": "0.18.0",
        "@remix-run/react": "2.6.0",
        "@remix-run/serve": "2.6.0",
        "cloudinary-build-url": "0.2.4",
        "clsx": "2.1.0",
        "cross-env": "7.0.3",
        "date-fns": "3.3.1",
        "isbot": "4.4.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "source-map-support": "0.5.21",
        "tiny-invariant": "1.3.1"
    },
    "devDependencies": {
        "@cld-apis/types": "0.1.6",
        "@netlify/plugin-lighthouse": "6.0.0",
        "@remix-run/dev": "2.6.0",
        "@remix-run/eslint-config": "2.6.0",
        "@remix-run/serve": "2.6.0",
        "@tailwindcss/aspect-ratio": "0.4.2",
        "@types/react": "18.2.55",
        "@types/react-dom": "18.2.19",
        "@types/source-map-support": "0.5.10",
        "eslint": "8.56.0",
        "husky": "9.0.10",
        "netlify-cli": "17.16.1",
        "prettier": "3.2.5",
        "prettier-plugin-tailwindcss": "0.5.11",
        "pretty-quick": "4.0.0",
        "rimraf": "5.0.5",
        "tailwindcss": "3.4.1",
        "tsx": "4.7.1",
        "typescript": "5.3.3"
    },
    "overrides": {
        "react": "$react",
        "react-dom": "$react-dom"
    },
    "packageManager": "pnpm@8.12.1",
    "keywords": [
        "remix",
        "yoga",
        "courses",
        "shopify"
    ],
    "license": "MIT",
    "engines": {
        "node": "18.x"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nicolaserny/yoga-by-delphine"
    },
    "bugs": {
        "url": "https://github.com/nicolaserny/yoga-by-delphine/issues"
    },
    "eslintIgnore": [
        "node_modules",
        "coverage",
        "build",
        "public/build"
    ]
}
